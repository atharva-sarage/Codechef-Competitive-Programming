#include<bits/stdc++.h>
using namespace std;
int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		int n,a,b,counta=0,countb=0;
		cin>>n>>a>>b;

		int x[10005];
		for(int i=0;i<n;i++)
			{
				cin>>x[i];
				if(x[i]==a)
					counta++;
				if(x[i]==b)
					countb++;
			}
		double ans=counta/(double)n * countb/(double)n;
		printf("%.9lf\n",ans );


	}
}