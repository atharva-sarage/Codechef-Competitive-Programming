#include<bits/stdc++.h>
using namespace std;
int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		double x1,x2,x3,v1,v2;
		cin>>x1>>x2>>x3>>v1>>v2;
		double ans1=(x3-x1)/v1;
		double ans2=(x2-x3)/v2;
		if(ans1==ans2)
			printf("Draw\n");
		else if(ans1>ans2)
		{
			printf("Kefa\n");
		}
		else
			printf("Chef\n");
	}
}