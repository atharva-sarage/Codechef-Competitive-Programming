#include<stdio.h>
int main()
{
	int t;
	scanf("%d",&t);
	while(t--)
	{
		int n,k,count=0;
		scanf("%d %d",&n,&k);
		if(n==1 && k==1)
			{
				printf("1\n");
				continue;
			}

		if(k>(n/2))
			printf("-1");
		else
		{
			int i=0;
			while(count<k )
			{
				i=i+2;
				printf("%d ",i);
				count++;

			}

		}
		printf("\n");


	}
}