#include<stdio.h>
int main()
{
	int t;
	scanf("%d",&t);
	while(t--)
	{

		int n,i,count1=0,count2=0;
		scanf("%d",&n);
		int a[1001]={0};
		for(i=0;i<n;i++)
		{
			scanf("%d",&a[i]);
			if(a[i]%2==0)
			{
				count1++;
			}
			else
			{
				count2++;
			}
		}
		if(count2%2==0)
			printf("1\n");
		else
			printf("2\n");

	}
}