#include<bits/stdc++.h>
using namespace std;
int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		char grid[4][4];
		for(int i=0;i<3;i++)
		{
			for(int j=0;j<3;j++)
			{
				cin>>grid[i][j];
			}
		}
		if(grid[0][0]=='l' && grid[1][0]=='l' && grid[1][1]=='l')
			{cout<<"yes"<<endl;continue;}
		if(grid[1][0]=='l' && grid[2][0]=='l' && grid[2][1]=='l')
			{cout<<"yes"<<endl;continue;}
		if(grid[0][1]=='l' && grid[1][1]=='l' && grid[1][2]=='l')
			{cout<<"yes"<<endl;continue;}
		if(grid[1][1]=='l' && grid[2][1]=='l' && grid[2][2]=='l')
			{cout<<"yes"<<endl;continue;}
		cout<<"no"<<endl;


	}
}