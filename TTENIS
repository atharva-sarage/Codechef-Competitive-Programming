#include<stdio.h>
#include<string.h>

	int main()
	{
		int t;
		scanf("%d",&t);
		while(t--)
		{
			int sum=0,i;
			char str[101];
			scanf("%s",str);
			int n=strlen(str);
			for(i=0;i<n;i++)
			{
				if(str[i]=='1')
				sum++;
							
			}
			if(sum>n-sum)
			printf("WIN\n");
			else
			printf("LOSE\n");
		}
	}
