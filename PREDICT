#include<stdio.h>
#include<math.h>
int main()
{
	int t;
	scanf("%d",&t);
	while(t--)
	{
		double p;
		scanf("%lf",&p);
		//printf("%f\n",((2.0)*p-1.0)*10000.0);
		//printf("%ld\n",fabs((2*p-1)*(10000))+10000);
		if(p+p>=1.0)
		{
			printf("%lf\n",((2.0)*p-1.0)*(1.0-p)*(10000.0)+10000.0);
		}
		else
		{
			printf("%lf\n",(1.0-2*p)*p*(10000.0)+10000.0);
		}
	}
}